/data/tools/riscv-embecosm-embedded-ubuntu2204-20240407-14.0.1/bin/riscv64-unknown-elf-gcc \
-march=rv64imacdf_zba_zbb_zbc_zbs_zfa \
-mabi=lp64d \
-nostdlib \
-nostartfiles \
-mcmodel=medany \
-O3 \
-ffast-math \
-funsafe-math-optimizations \
-finline-functions \
-fno-common \
-fno-builtin-printf \
-flto \
-fno-tree-loop-distribute-patterns \
-I/data/users/jeffnye/condor/benchmarks/microbench/common \
-DPREALLOCATE=1 \
-D__riscv=1 \
-Dzba_ENABLED=1 \
-Dzbb_ENABLED=1 \
-Dzbc_ENABLED=1 \
-Dzbs_ENABLED=1 \
-static \
-std=gnu99 \
-T/data/users/jcatch_alleffnye/condor/benchmarks/linker/common_proposed.ld \
/data/users/jeffnye/condor/benchmarks/microbench/common/syscalls.c \
./crt.S \
./sanity.c \
-o ./astar.riscv \
-lm \
-lgcc

/data/tools/riscv-embecosm-embedded-ubuntu2204-20240407-14.0.1/bin/riscv64-unknown-elf-objdump --disassemble-all --disassemble-zeroes --section=.text \
--section=.text.startup --section=.text.init  --section=.data \
-Mnumeric,no-aliases ./astar.riscv > astar.objdump

$TOP/cpm.dromajo/build/dromajo  \
--march=rv64gc_zba_zbb_zbc_zbs_zfa \
--stf_priv_modes USHM \
--stf_trace=sanity.zstf \
--stf_exit_on_stop_opc \
--ctrlc \
--march=rv64gc \
./astar.riscv
